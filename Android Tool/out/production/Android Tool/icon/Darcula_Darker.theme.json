{
  "name": "Darcula Darker",
  "dark": true,
  "author": "Vecheslav Druzhbin",
  "editorScheme": "/Darcula_Darker.xml",
  "ui": {
    "*": {
      "background": "#2f3032",
      "disabledBackground": "#2F3032",
      "inactiveBackground": "#333436",

      "selectionBackground": "#2f65ca",
      "selectionInactiveBackground": "#2D384C",

      "lightSelectionBackground": "#393a3d",
      "lightSelectionInactiveBackground": "#2f3032",

      "borderColor": "#414245",
      "disabledBorderColor": "#333336",

      "separatorForeground": "#414245",
      "separatorColor": "#414245",
      "lineSeparatorColor": "#414245"
    },

    "Borders": {
      "color": "#414245",
      "ContrastBorderColor": "#414245"
    },

    "Tree.rowHeight": 20,

    "VersionControl": {
      "Log.Commit": {
        "currentBranchBackground": "#2E3D5A",
        "unmatchedForeground": "#727380"
      },
      "FileHistory.Commit.selectedBranchBackground": "#2E3D5A"
    },

    "DefaultTabs": {
      "hoverBackground": "#00000000",
      "underlineColor": "#2f65ca",
      "underlinedTabBackground": "#393a3d"
    },

    "SearchEverywhere": {
      "Tab": {
        "selectedForeground": "#CFD2D5",
        "selectedBackground": "#393a3d"
      }
    },

    "EditorTabs": {
      "selectedBackground": "#37383b",
      "inactiveMaskColor": "#00000033",
      "underlinedTabBackground": "#393a3d",
      "underlinedTabForeground": "#CFD2D5"
    },

    "ToolWindow.HeaderTab": {
      "underlinedTabBackground": "#393a3d"
    }
  }
}